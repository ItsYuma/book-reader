<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="GBReaderStefkoS.Avalonia.UserControls.SessionUserControl">
    <Border BorderBrush="Black"
            BorderThickness="2"
            CornerRadius="3"
            Padding="4"
            Margin="4"
            Width="400">
        <StackPanel>
            <TextBlock x:Name="BookTitle"></TextBlock>
            <TextBlock x:Name="BookIsbn"></TextBlock>
            <TextBlock x:Name="PageIndex"></TextBlock>
            <TextBlock x:Name="DateStart"></TextBlock>
            <TextBlock x:Name="DateLastReading"></TextBlock>
        </StackPanel>
    </Border> 
</UserControl>
